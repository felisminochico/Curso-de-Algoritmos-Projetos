algoritmo "ConversordeTemperatura"
// Função : Convertendo Temperatura
// Autor : Felismino Chico
// Data : 31/10/2022
// Seção de Declarações 
var
   F, C: Real
inicio
      Escreva("Qual é a minha temperatura aqui? ")
      Leia(F)
      C <- (F- 32) / 1.8
      Escreva("A tua temperatura será de ", C:4:1)
fimalgoritmo
