algoritmo "Soma"
// Função :
// Autor :
// Data : 05/12/2022
// Seção de Declarações
var
   X, Y: Inteiro
   Procedimento Soma(A, B: Inteiro)
   Inicio
         Escreval("O valor de A atribuído é ", A)
         Escreval("O valor de b atribuído é ", B)
         Escreva("A soma entre eles é ", A + B)
   FimProcedimento
inicio
      X <- 5
      Y <- 2
      Soma(X, Y)
fimalgoritmo
